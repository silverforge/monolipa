{
  "name": "MoNoLiPa",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start-ios": "npm run compile && react-native run-ios",
    "copy_config": "node copyConfig.js",
    "compile": "npm run delete_dist && tsc || true && node copyConfig.js",
    "delete_dist": "rm -rf lib",
    "test": "jest",
    "android-deploy-to-device": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android"
  },
  "dependencies": {
    "react": "16.2.0",
    "react-native": "0.53.0"
  },
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/react": "^16.0.38",
    "@types/react-native": "^0.52.13",
    "@types/react-test-renderer": "^16.0.1",
    "babel-jest": "22.2.2",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.3.0",
    "react-test-renderer": "16.2.0",
    "shelljs": "^0.8.1",
    "ts-jest": "^22.0.4",
    "typescript": "^2.7.2"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/**/"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
